   "source": [
    "import pandas as pd\n",
    "\n",
    "def add_word_count_column(df):\n",
    "    # Check if 'Text' column exists in the DataFrame\n",
    "    if 'Text' in df.columns:\n",
    "        # Create 'Word_Count' column by applying a function to count words in each row of 'Text' column\n",
    "        df['Word_Count'] = df['Text'].apply(lambda x: len(str(x).split()))\n",
    "    else:\n",
    "        print(\"The DataFrame does not have a 'Text' column.\")\n",
    "    return df\n",
    "\n",
